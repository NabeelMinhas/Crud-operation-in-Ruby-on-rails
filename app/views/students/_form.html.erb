<div class="container mt-5">
  <%= form_with model: @student do |forms| %>
  <div>
    <%= forms.label :name, {class:"form-label"} %><br>
    <%= forms.text_field :name, {class: "form-control"} %>
  </div>

  <div>
    <%= forms.label :rollnumber, {class:"form-label"} %><br>
    <%= forms.text_field :rollnumber, {class: "form-control"} %>
  </div>

  <div class="text-center">
    <%= forms.submit "Create", {class:"btn btn-primary mt-5"} %>
  </div>
<% end %>
</div>
